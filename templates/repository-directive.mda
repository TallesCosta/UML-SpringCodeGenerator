import java.util.*
import in.labulle.anycode.uml.*
import in.labulle.anycode.engine.groovy.directive.*

class Repository {
	IClass c;

	def setup(IClass c) {
		this.c = c;
	}

	def imports() {
		String imports = ""

		imports += "\nimport ${c.owner.getFullyQualifiedName(".")}.dominio.${c.name};\n"
		imports += "\nimport org.springframework.data.repository.CrudRepository;\n"

		return imports
	}

}